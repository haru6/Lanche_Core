<li class="dropdown mt-2">
    <a asp-controller="Snack" asp-action="List" class="dropdown-toggle" data-toggle="dropdown" style="color:black">Lanches</a>
    <ul class="dropdown-menu">
        @foreach (var category in Model)
        {
            <li>
                <a asp-controller="Snack" asp-action="List" style="color:black"
                   asp-route-category="@category.CateoryName">@category.CateoryName</a>
            </li>
        }
        <li class="dropdown-divider"></li>
        <li>
            <a asp-controller="Snack" asp-action="List" asp-route-category="" style="color:black">Todos os Lanches</a>
        </li>
    </ul>
</li>